{
  "name": "Pre-Style-css-in-js",
  "version": "1.0.0",
  "description": "Pre-Style - CSS in JS",
  "scripts": {
    "build": "webpack"
  },
  "author": "Benjamin Solum",
  "license": "MIT",
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "pre-style",
        {
          "config": "PreStyleConfig.js"
        }
      ]
    ]
  },
  "postcss": {
    "plugins": {
      "cssnano": {
        "autoprefixer": false,
        "discardComments": {
          "removeAll": true
        }
      },
      "autoprefixer": {
        "remove": false
      }
    }
  },
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-pre-style": "^1.0.0",
    "css-loader": "^3.1.0",
    "cssnano": "^4.1.10",
    "mini-css-extract-plugin": "^0.8.0",
    "postcss-loader": "^3.0.0",
    "sass": "^1.22.9",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.39.0",
    "webpack-cli": "^3.3.12"
  }
}
